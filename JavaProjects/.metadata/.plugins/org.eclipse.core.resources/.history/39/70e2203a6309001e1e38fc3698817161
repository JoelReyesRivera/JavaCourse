
public class Main {
	
	public static void main(String[] args) {
		//TIPOS DE DATOS PRIMITIVOS
		//Numeros enteros
		int entero = 10;
		short corto = 32767;
		long largo = 1000000L;
		byte bytePequeno = 1;
		int dato; //Si la variable no se inicializa, el valor es 0
		
		//Numeros punto flotante
		float flotante = 3.14f;
		double doble = 2.71828;
		double dato1;//Si la variable no se inicializa, el valor es 0.0
		
		//Caracteres
		char caracter = 'A';
		char carater1;//Si la variable no se inicializa, el valor es 0.0
		System.out.println()carater1;
		//Booleanos 
		boolean verdadero = true;
		boolean falso = false;

		//TIPOS DE DATO NO PRIMITIVOS
		//String
		String cadena = "12k3o12k3!#!$%!#$!!3";
	}
	
	public void MetodoVoid() {
		System.out.println("Impresion en metodo void");
	}
	
	public int MetodoInt() {
		return 0;
	}
	
	public String MotodoString () {
		return "Cadena";
	}
	
	public double MotodoFlotante () {
		return .10;
	}
	

}
