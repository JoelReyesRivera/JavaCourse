
public class Estructuras {

	public static void main(String[] args) {
		
		//ESTRUCTURA IF
		if (true) {
			
		}
		
		//FOR
		for (int i = 0; i < 10; i++) {
			
		}
		
		//WHILE
		while (true) {
			
		}
		
		//SWITCH
		
		switch (true) {
		case true:
			
			break;

		default:
			break;
		}
		
	}
}
